<div class="small-panel upload-image-panel">
  <div class="uploaded-image-main row">
    <div class="active-image">
      <img src="<%= image.escape("url") %>" alt="" />
    </div>
  </div>
  <div class="thumbnail-images row">
    <% transformedUrls.forEach(function(transformedUrl){ %>
      <div class="gutter-image col-xs-2 col-md-2">
        <img src="<%= transformedUrl %>" alt="" />
      </div>
    <%  }) %>
  </div>
  <form class="upload-form">
    <strong>Caption</strong> <br>
    <input type="text" name="caption" placeholder="Add a caption..."><br><br>
    <strong>Location</strong> <br>
    <input type="text" name="location" placeholder="Add a location..."><br><br>
    <button type="button" name="cancel" class="btn-danger">Cancel</button>
    <input type="submit" value="Share" class="btn-success">
  </form>
</div>
