<div class="landing-content signin-landing">
  <div class="landing-header">
    <div class="welcome-logo">
      <img src="/Images/Wandergram-logo.png" alt="" />
    </div>
  </div>
  <div class="landing-form-container signin-landing-container">
    <div class="sign-up-form">
      <div class="form-body">
        <form action="<%= session_url %>" method="post">
          <%= auth_token %>
          <input class="landing-input" type="text" name="user[username]" id="user_username" placeholder="Username">
          <br><br>
          <input class="landing-input" type="password" name="user[password]" id="user_password" placeholder="Password">
          <br><br>
          <input type="submit" class="btn-success signup" value="Log In">
        </form>

      </div>
    </div>
    <div class="log-in-footer">
      <div class="form-footer-text">
        Don't have an account? <br>
        <a href="<%= new_user_url %>">Click here to sign up</a>
      </div>
    </div>
    <div>
    </div>

</div>
